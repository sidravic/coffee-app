<!DOCTYPE html>
<html>
<head>
  <title>CoffeeApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div class='header'>
      <h1> Coffee Shop App</h1>
    </div>

    <%= render partial: 'layouts/flash_messages' %>

    <div class='left'>
      <%= yield %>
    </div>

    <div class='right'>
      <%= yield(:right) %>
    </div>

    <div class='clear'>
    </div>


    <div class='footer'>
      <small> Copyrights 2015</small>
    </div>
</body>
</html>
